	let SelectCategorie = "Main";
	let UseListButton = -1;
	let isUpdate = false;

	const OnChangeList = (type, change) => {
		$$invalidate(0, SettingsList[type].selectId += change, SettingsList);
		let appearancesList = SettingsList[type].list;
		if (SettingsList[type].selectId >= appearancesList.length) $$invalidate(0, SettingsList[type].selectId = 0, SettingsList); else if (SettingsList[type].selectId < 0) $$invalidate(0, SettingsList[type].selectId = appearancesList.length - 1, SettingsList);
		$$invalidate(14, isUpdate = true);
	};

	const OnSwitch = (type, change) => {
		$$invalidate(0, SettingsList[type].toggled = !SettingsList[type].toggled, SettingsList);
		$$invalidate(14, isUpdate = true);

		if (type === "HudToggled") {
			$$invalidate(0, SettingsList["ChatOpacity"].value = 50, SettingsList);
			$$invalidate(0, SettingsList["HudStats"].toggled = $$invalidate(0, SettingsList["HudSpeed"].toggled = $$invalidate(0, SettingsList["HudOnline"].toggled = $$invalidate(0, SettingsList["HudLocation"].toggled = $$invalidate(0, SettingsList["HudKey"].toggled = $$invalidate(0, SettingsList["HudMap"].toggled = $$invalidate(0, SettingsList["HudCompass"].toggled = SettingsList["HudToggled"].toggled, SettingsList), SettingsList), SettingsList), SettingsList), SettingsList), SettingsList), SettingsList);
		} else if (type === "HudStats" || type === "HudSpeed" || type === "HudOnline" || type === "HudLocation" || type === "HudKey" || type === "HudMap" || type === "HudCompass" || type === "ChatOpacity") {
			$$invalidate(0, SettingsList["HudToggled"].toggled = true, SettingsList);
		}
	};

	const OnProgress = (type, value) => {
		$$invalidate(0, SettingsList[type].value = value, SettingsList);
		$$invalidate(14, isUpdate = true);
	};

	(0,svelte__WEBPACK_IMPORTED_MODULE_9__.onDestroy)(() => {
		SaveData();
	});

	let { visible } = $$props;
	let saveDataDefault = {};

	store_settings__WEBPACK_IMPORTED_MODULE_4__.storeSettings.subscribe(value => {
		if (saveDataDefault !== value) {
			saveDataDefault = value;

			for (let key in saveDataDefault) {
				if (SettingsList[key]) {
					if (SettingsList[key].list !== undefined && SettingsList[key].selectId !== undefined) $$invalidate(0, SettingsList[key].selectId = saveDataDefault[key], SettingsList); else if (SettingsList[key].toggled !== undefined) $$invalidate(0, SettingsList[key].toggled = saveDataDefault[key], SettingsList); else if (SettingsList[key].min !== undefined && SettingsList[key].max !== undefined) $$invalidate(0, SettingsList[key].value = saveDataDefault[key], SettingsList);
				}
			}
		}
	});

	const SaveData = () => {
		let saveData = {};

		Object.values(SettingsList).forEach(item => {
			if (item.list !== undefined && item.selectId !== undefined) saveData[item.type] = item.selectId; else if (item.toggled !== undefined) saveData[item.type] = item.toggled; else if (item.min !== undefined && item.max !== undefined) saveData[item.type] = item.value;
		});

		(0,api_rage__WEBPACK_IMPORTED_MODULE_5__.executeClient)("chatconfig", JSON.stringify(saveData));
	};

	const onDefaultButton = () => {
		const DefaultAllAccess = {
			Timestamp: false, // 0
			ChatShadow: true,
			APunishments: false, // 0
			Fontsize: 16, // 16
			ChatOpacity: 50, // 100
			Chatalpha: 1, // 100            
