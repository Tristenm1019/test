				clickTime = new Date().getTime() + 1000;

				$$invalidate(9, selectItem = {
					...getItemToIndex(index, arrayName),
					use: _functions_js__WEBPACK_IMPORTED_MODULE_5__.stageItem.get,
					width: target.width,
					height: target.height,
					offsetInElementX,
					offsetInElementY,
					clientX: event.clientX,
					clientY: event.clientY,
					index,
					arrayName
				});

				mouseLeaveSelectedItem = false;
			}
		} else if (event.which == 3 && (arrayName !== "other" || arrayName === "other" && OtherInfo.Id !== _functions_js__WEBPACK_IMPORTED_MODULE_5__.otherType.Nearby && OtherInfo.Id !== _functions_js__WEBPACK_IMPORTED_MODULE_5__.otherType.Tent) && ItemsData[arrayName][index].ItemId != 0 && getItemToIndex(index, arrayName).use) {
			const item = getItemToIndex(index, arrayName);

			const _selectItem = {
				...item,
				use: _functions_js__WEBPACK_IMPORTED_MODULE_5__.stageItem.useItem,
				index,
				arrayName
			};

			if (getItemsUse(selectItem) === false && OtherInfo.Id <= _functions_js__WEBPACK_IMPORTED_MODULE_5__.otherType.None && item.Count <= 0 && getDropItem(arrayName, item.ItemId) === false) return;
			unHoverAll();
			updateItem(index, arrayName, "hover", true);
			$$invalidate(10, infoItem = _functions_js__WEBPACK_IMPORTED_MODULE_5__.defaulHoverItem);
			coords.set({ x: event.clientX, y: event.clientY });
			$$invalidate(9, selectItem = _selectItem);
		}
	};

	const handleInputChange = (name, value) => {
		value = Math.round(value.replace(/\D+/g, ""));
		if (value < 0) value = 0; else if (value > 9999999) value = 9999999;
		$$invalidate(13, tradeInfo[name] = value, tradeInfo);
		if (name === "YourMoney") (0,api_rage__WEBPACK_IMPORTED_MODULE_3__.executeClient)("client.gamemenu.inventory.tradeMoney", value);
	};

	const onBlur = () => {
		if (tradeInfo.YourMoney < 0) {
			window.notificationAdd(4, 9, (0,lang__WEBPACK_IMPORTED_MODULE_1__.translateText)('player1', 'Сумма не может быть меньше 0.'), 3000);
			$$invalidate(13, tradeInfo.YourMoney = 0, tradeInfo);
		} else if (tradeInfo.YourMoney > 9999999) {
			window.notificationAdd(4, 9, (0,lang__WEBPACK_IMPORTED_MODULE_1__.translateText)('player1', 'Сумма не может быть больше $9.999.999.'), 3000); //return;
			$$invalidate(13, tradeInfo.YourMoney = 9999999, tradeInfo);
		} else if (Number($charMoney) < tradeInfo.YourMoney) {
			$$invalidate(13, tradeInfo.YourMoney = Number($charMoney), tradeInfo); //return;
			window.notificationAdd(4, 9, `${(0,lang__WEBPACK_IMPORTED_MODULE_1__.translateText)('player1', 'Сумма не может быть больше')} ${(0,api_formatter__WEBPACK_IMPORTED_MODULE_6__.format)("money", tradeInfo.YourMoney)}.`, 3000);
		} //return;

		(0,api_rage__WEBPACK_IMPORTED_MODULE_3__.executeClient)("client.gamemenu.inventory.tradeMoney", Math.round(tradeInfo.YourMoney));
	};

	//Глобальные
	const handleGlobalMouseMove = event => {
		if (!visible) return; else if (isMoveBlock) {
			$$invalidate(
				11,
				moveBlock[isMoveBlock] = [
					event.clientY - selectItem.offsetInElementY,
					event.clientX - selectItem.offsetInElementX
				],
				moveBlock
			);

			if (moveBlock[isMoveBlock][0] + selectItem.height > window.innerHeight) $$invalidate(11, moveBlock[isMoveBlock][0] = window.innerHeight - selectItem.height, moveBlock); else if (moveBlock[isMoveBlock][0] < 0) $$invalidate(11, moveBlock[isMoveBlock][0] = 0, moveBlock);
			if (moveBlock[isMoveBlock][1] + selectItem.width > window.innerWidth) $$invalidate(11, moveBlock[isMoveBlock][1] = window.innerWidth - selectItem.width, moveBlock); else if (moveBlock[isMoveBlock][1] < 0) $$invalidate(11, moveBlock[isMoveBlock][1] = 0, moveBlock);
		} else if (infoItem !== _functions_js__WEBPACK_IMPORTED_MODULE_5__.defaulHoverItem) {
			$$invalidate(16, boxInfoLeft = fixOutToCenter($coords.x, boxItemInfo));
			$$invalidate(17, boxInfoTop = fixOutToTop($coords.y, boxItemInfo));
		} else if (selectItem.use === _functions_js__WEBPACK_IMPORTED_MODULE_5__.stageItem.move && infoItem === _functions_js__WEBPACK_IMPORTED_MODULE_5__.defaulHoverItem || selectItem.use !== _functions_js__WEBPACK_IMPORTED_MODULE_5__.stageItem.useItem && selectItem.use !== _functions_js__WEBPACK_IMPORTED_MODULE_5__.stageItem.get && infoItem === _functions_js__WEBPACK_IMPORTED_MODULE_5__.defaulHoverItem) {
			let clientX = event.clientX;
			let clientY = event.clientY;
			if (clientY + selectItem.height > window.innerHeight) clientY = window.innerHeight - selectItem.height; else if (clientY < 0) clientY = 0;
			if (clientX + selectItem.width > window.innerWidth) clientX = window.innerWidth - selectItem.width; else if (clientX < 0) clientX = 0;
			coords.set({ x: clientX, y: clientY });
		} else if (selectItem.use === _functions_js__WEBPACK_IMPORTED_MODULE_5__.stageItem.get && (selectItem.clientX !== event.clientX || selectItem.clientY !== event.clientY)) {
			unHoverAll();
			$$invalidate(9, selectItem = { ...selectItem, use: _functions_js__WEBPACK_IMPORTED_MODULE_5__.stageItem.move });
			let clientX = event.clientX;
			let clientY = event.clientY;
			if (clientY + selectItem.height > window.innerHeight) clientY = window.innerHeight - selectItem.height; else if (clientY < 0) clientY = 0;
			if (clientX + selectItem.width > window.innerWidth) clientX = window.innerWidth - selectItem.width; else if (clientX < 0) clientX = 0;
			coords.set({ x: clientX, y: clientY });
		}
	};

	const onUseItem = () => {
