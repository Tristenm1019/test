function instance($$self, $$props, $$invalidate) {
	let { selectedProductName } = $$props;
	let { onSelectedViewProduct } = $$props;
	let { onSelectedProductName } = $$props;
	let product = {};

	const updateData = () => {
		(0,api_rage__WEBPACK_IMPORTED_MODULE_2__.executeClientAsyncToGroup)("business.getProduct", selectedProductName).then(result => {
			if ((0,api_functions__WEBPACK_IMPORTED_MODULE_3__.hasJsonStructure)(result)) $$invalidate(1, product = JSON.parse(result));
		});
	};

