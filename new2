	let bizType = 0;

	(0,api_rage__WEBPACK_IMPORTED_MODULE_2__.executeClientAsyncToGroup)("business.getType").then(result => {
		$$invalidate(2, bizType = result);
	});

	let price;

	const onSelect = () => {
		if (!window.loaderData.delay("business.extraCharge", 1.5)) return;
		const value = Number(price);

		if (product.minPrice > value || value > product.maxPrice) {
			window.notificationAdd(4, 9, `${(0,lang__WEBPACK_IMPORTED_MODULE_1__.translateText)('player2', 'Наценка не может быть меньше')} ${product.minPrice}${(0,_data__WEBPACK_IMPORTED_MODULE_4__.getPrefix)(selectedProductName, bizType)} и больше чем ${product.maxPrice}${(0,_data__WEBPACK_IMPORTED_MODULE_4__.getPrefix)(selectedProductName, bizType)}`, 3000);
			return;
		}

		(0,api_rage__WEBPACK_IMPORTED_MODULE_2__.executeClientToGroup)("business.extraCharge", selectedProductName, value);
	};

	const onSelectBack = () => {
		onSelectedViewProduct("Item");
	};

	(0,svelte__WEBPACK_IMPORTED_MODULE_7__.onDestroy)(() => {
		(0,_views_player_hudevo_phonenew_data__WEBPACK_IMPORTED_MODULE_6__.onInputBlur)();
	});

