function instance($$self, $$props, $$invalidate) {
	let { selectedProductName } = $$props;
	let { onSelectedViewProduct } = $$props;
	let { onSelectedProductName } = $$props;
	let product = {};

	const updateData = () => {
		(0,api_rage__WEBPACK_IMPORTED_MODULE_3__.executeClientAsyncToGroup)("business.getProduct", selectedProductName).then(result => {
			if ((0,api_functions__WEBPACK_IMPORTED_MODULE_4__.hasJsonStructure)(result)) $$invalidate(0, product = JSON.parse(result));
		});
	};

	updateData();
	(0,api_functions__WEBPACK_IMPORTED_MODULE_4__.addListernEvent)("phoneBusinessUpdate", updateData);

	const getExtraCharge = (bType, price, defaultPrice) => {
		if (bType == 7 || bType == 9 || bType == 10 || bType == 11 || bType == 12) {
			return `${price}%`;
		}

		return `${Math.round(price * 100 / defaultPrice)}% ($${(0,api_formatter__WEBPACK_IMPORTED_MODULE_2__.format)("money", price)})`;
	};

	const onChangePrice = () => {
		onSelectedViewProduct("ChangePrice");
	};

	const onOrderProduct = () => {
		onSelectedViewProduct("OrderProduct");
	};

	const onSelectBack = () => {
		onSelectedViewProduct("List");
		onSelectedProductName("");
	};

	let bizType = 0;

	(0,api_rage__WEBPACK_IMPORTED_MODULE_3__.executeClientAsyncToGroup)("business.getType").then(result => {
		$$invalidate(1, bizType = result);
	});

	const cancelOrder = () => {
		if (!window.loaderData.delay("business.cancelOrder", 1.5)) return;
		(0,api_rage__WEBPACK_IMPORTED_MODULE_3__.executeClientToGroup)("business.cancelOrder", product.uidOrder);
	};

	$$self.$$set = $$props => {
		if ('selectedProductName' in $$props) $$invalidate(7, selectedProductName = $$props.selectedProductName);
		if ('onSelectedViewProduct' in $$props) $$invalidate(8, onSelectedViewProduct = $$props.onSelectedViewProduct);
		if ('onSelectedProductName' in $$props) $$invalidate(9, onSelectedProductName = $$props.onSelectedProductName);
	};

	return [
		product,
		bizType,
		getExtraCharge,
		onChangePrice,
		onOrderProduct,
		onSelectBack,
		cancelOrder,
		selectedProductName,
		onSelectedViewProduct,
		onSelectedProductName
	];
}

