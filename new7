				let color = api_tinycolor_min_js__WEBPACK_IMPORTED_MODULE_2___default()('rgb ' + red.value + ' ' + green.value + ' ' + blue.value);
				colorToPos(color);
			});
		}

		/*addSwatch.addEventListener('click', function(){  
createSwatch(userSwatches, currentColor);
});*/
		new ColorPicker();

		setTimeout(
			() => {
				if (defaultColor && defaultColor.length) {
					let color = api_tinycolor_min_js__WEBPACK_IMPORTED_MODULE_2___default()('rgb ' + defaultColor[0] + ' ' + defaultColor[1] + ' ' + defaultColor[2]);

