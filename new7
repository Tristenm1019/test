		(0,api_rage__WEBPACK_IMPORTED_MODULE_4__.executeClientAsyncToGroup)("getSettings").then(result => {
			if (result && typeof result === "string") $$invalidate(4, settings = JSON.parse(result));
		});
	};

	getSettings();

	function input_input_handler() {
		searchText = this.value;
		$$invalidate(3, searchText);
	}

	const click_handler = item => onSelectRankId(Number(item.id));
	const func = el => filterCheck(el.name, searchText);
	const click_handler_1 = item => onSelectRank(item.uuid);
	const click_handler_2 = () => (0,_data__WEBPACK_IMPORTED_MODULE_3__.setPopup)();

	return [
		members,
		selectRankId,
		ranksData,
