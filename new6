function instance($$self, $$props, $$invalidate) {
	let { settings } = $$props;
	let { onSettings } = $$props;

	//search
	let searchText = "";

	const filterCheck = (elText, text) => {
		if (!text || !text.length) return true;
		text = text.toUpperCase();
		if (elText.toString().toUpperCase().includes(text)) return true;
		return false;
	};

	let selectRank = null;

	const onSelectRank = (item = null) => {
		$$invalidate(2, selectRank = item);
	};

	const click_handler = () => onSettings();

	function input_input_handler() {
		searchText = this.value;
		$$invalidate(3, searchText);
	}

	const func = el => filterCheck(el.name, searchText);
	const click_handler_1 = item => onSelectRank(item);

	$$self.$$set = $$props => {
		if ('settings' in $$props) $$invalidate(0, settings = $$props.settings);
		if ('onSettings' in $$props) $$invalidate(1, onSettings = $$props.onSettings);
	};

	$$self.$$.update = () => {
		if ($$self.$$.dirty & /*settings, selectRank*/ 5) {
			$: if (settings) {
				if (selectRank) {
					const index = settings.ranks.findIndex(a => a.id === selectRank.id);
					if (settings.ranks[index]) onSelectRank(settings.ranks[index]);
				}
			}
		}
	};

	return [
		settings,
		onSettings,
		selectRank,
		searchText,
		filterCheck,
		onSelectRank,
		click_handler,
		input_input_handler,
		func,
		click_handler_1
	];
}

