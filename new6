			$$invalidate(1, dragonDropItem = item1);
			$$invalidate(0, ranks[index2] = item1, ranks);
		}
	};

	let isSave = false;

	const saveRanks = () => {
		if (isSave) return;
		isSave = true;
		const updateRankId = {};

		ranks.forEach(item => {
			if (typeof item.oldId !== "undefined" && item.oldId !== item.id) {
				updateRankId[item.oldId] = item.id;
			}
		});

		if (updateRankId && Object.keys(updateRankId).length) (0,api_rage__WEBPACK_IMPORTED_MODULE_2__.executeClientToGroup)('updateRanksId', JSON.stringify(updateRankId));
