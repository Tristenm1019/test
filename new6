function instance($$self, $$props, $$invalidate) {
	let $coords;
	let { ranks } = $$props;

	const onCreateCallback = (rankName, score) => {
		let check = (0,api_formatter__WEBPACK_IMPORTED_MODULE_4__.format)("rank", rankName);

		if (!check.valid) {
			window.notificationAdd(4, 9, check.text, 3000);
			return;
		}

		score = Math.round(score.toString().replace(/\D+/g, ""));
		if (score < 0) score = 0; else if (score > 9999999) score = 9999999;
		(0,api_rage__WEBPACK_IMPORTED_MODULE_2__.executeClientToGroup)('createRank', rankName, score);
	};

	const onCreate = () => {
		(0,_views_player_menu_elements_fractions_data_js__WEBPACK_IMPORTED_MODULE_3__.setPopup)("Input", {
