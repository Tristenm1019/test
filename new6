		i: svelte_internal__WEBPACK_IMPORTED_MODULE_0__.noop,
		o: svelte_internal__WEBPACK_IMPORTED_MODULE_0__.noop,
		d(detaching) {
			if (detaching) (0,svelte_internal__WEBPACK_IMPORTED_MODULE_0__.detach)(div24);
			(0,svelte_internal__WEBPACK_IMPORTED_MODULE_0__.destroy_each)(each_blocks, detaching);
			mounted = false;
			(0,svelte_internal__WEBPACK_IMPORTED_MODULE_0__.run_all)(dispose);
		}
	};
}

function instance($$self, $$props, $$invalidate) {
	let { departmentId } = $$props;
	let { members } = $$props;
	let { onSelectMember } = $$props;
	let { onlineData } = $$props;
	let { ranks } = $$props;
	let searchText = "";

	const filterCheck = (elText, text) => {
		if (!text || !text.length) return true;
		text = text.toUpperCase();
		if (elText.toString().toUpperCase().includes(text)) return true;
		return false;
	};

	let deletePlayerUUid = 0;

	const onPlayerDeleteCallback = () => {
		(0,api_rage__WEBPACK_IMPORTED_MODULE_3__.executeClientToGroup)("deletePlayerDepartment", departmentId, deletePlayerUUid);
	};

	const onDepartmentDelete = item => {
		deletePlayerUUid = item.uuid;

		(0,_views_player_menu_elements_fractions_data_js__WEBPACK_IMPORTED_MODULE_2__.setPopup)("Confirm", {
			headerTitle: "Убрать из отряда",
			headerIcon: "fractionsicon-members",
